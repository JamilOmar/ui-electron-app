{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ui-app-04": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@labshare/shell-ui:browser",
          "options": {
            "outputPath": "dist/ui-app-04",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@labshare/shell-ui:dev-server",
          "options": {
            "browserTarget": "ui-app-04:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ui-app-04:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ui-app-04:build"
          }
        },
        "test": {
          "builder": "@labshare/shell-ui:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ui-app-04:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ui-app-04:serve:production"
            }
          }
        },
        "electron": {
          "builder": "@labshare/shell-ui:electron",
          "options": {
            "browserTarget": "ui-app-04:build",
            "baseHref": "./",
            "main":"electron-main.js",
            "outputPath": "dist/ui-app-04/electron-app",
            "useLocal" :true
          }
        },
        "pack-electron": {
          "builder": "@labshare/shell-ui:pack-electron",
          "options": {
            "browserTarget": "ui-app-04:build",
            "outputPath": "dist/ui-app-04/electron",
            "plaform": "mac",
            "baseHref": "./",
            "electronBuilder":{
              "buildVersion" :"v.1.1.10",
              "dmg": {
                "contents": [
                  {
                    "x": 110,
                    "y": 150
                  },
                  {
                    "x": 240,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                  }
                ],
                "sign":true
              },
              "mac":{
                "target": "dmg"
              },
              "publish": [{
                "provider": "github",
                "owner": "jamilomar",
                "repo": "ui-electron-app",
                "releaseType" : "release"}]
            },
            "publish" : "always"
          }
        },
        "pack-cordova": {
          "builder": "@labshare/shell-ui:pack-cordova",
          "options": {
            "browserTarget": "ui-app-04:build",
            "outputPath": "dist/ui-app-04/cordova",
            "plaform": "ios",
            "baseHref": "./",
            "cordovaAssets": false
          }
        },
        "pack-sharepoint": {
          "builder": "@labshare/shell-ui:pack-sharepoint",
          "options": {
            "browserTarget": "ui-app-04:build",
            "baseHref": "./",
            "outputPath": "dist/ui-app-04/sharepoint",
            "spVersion": "online",
            "sharepoint": {
              "solutionName": "ui-app-04",
              "solutionGuid": "4c874b09-7940-4c1c-9203-d1b6c3f1d74d",
              "zippedPackage": "solution/ui-app-04.sppkg",
              "webPartGuid": "c3ad6298-a085-4269-9b94-357640cdcf69",
              "webPartAlias": "ui-app-04",
              "webPartTitle": "ui-app-04",
              "webPartDescription": "ui-app-04 description",
              "webPartPropertiesDescription": "ui-app-04 properties description"
            }
          }
        }
      }
    }
  },
  "defaultProject": "ui-app-04"
}